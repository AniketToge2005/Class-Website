 <!-- CSS Styling -->
<style>
  .card_box {
    background: #fff;
    border-radius: 12px;
    box-shadow: 0 6px 20px rgba(0,0,0,0.1);
    padding: 20px;
    text-align: center;
    transition: 0.3s ease;
  }

  .card_box:hover {
    transform: translateY(-5px);
    box-shadow: 0 10px 25px rgba(0,0,0,0.15);
  }

  .image_box {
    height: 200px;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-bottom: 20px;
  }

  .image_box img {
    max-width: 100%;
    max-height: 100%;
    object-fit: contain;
    border-radius: 8px;
  }

  .sound_text {
    font-size: 20px;
    font-weight: 600;
    margin-bottom: 15px;
    color: #333;
  }

  .buy_bt a {
    background-color: #ff5e14;
    color: white;
    padding: 8px 20px;
    border-radius: 5px;
    text-decoration: none;
    font-weight: 500;
  }

  .buy_bt a:hover {
    background-color: #e04e0f;
  }

  @media (max-width: 768px) {
    .image_box {
      height: 160px;
    }

    .sound_text {
      font-size: 18px;
    }
  }
</style>

