  <!-- Tabs Script -->
  <script>
    // Tab functionality
    document.addEventListener('DOMContentLoaded', () => {
      const tabs = document.querySelectorAll('.grade-tab');
      tabs.forEach(tab => {
        tab.addEventListener('click', () => {
          document.querySelectorAll('.grade-tab').forEach(t => t.classList.remove('active'));
          document.querySelectorAll('.grade-content').forEach(c => c.classList.remove('active'));
          tab.classList.add('active');
          document.getElementById(tab.dataset.tab).classList.add('active');
        });
      });
      
      // Contact form submission
      const contactForm = document.getElementById('contactForm');
      if (contactForm) {
        contactForm.addEventListener('submit', function(e) {
          e.preventDefault();
          // In a real application, you would send the form data to a server here
          document.getElementById('successMessage').style.display = 'block';
          contactForm.reset();
          setTimeout(() => {
            document.getElementById('successMessage').style.display = 'none';
          }, 5000);
        });
      }
    });
    
    // Sample program data
    const programData = {
      'math-middle': {
        title: 'Mathematics Mastery (5th-8th Grade)',
        content: `<p>Our middle school math program focuses on building strong foundational skills while making math engaging and relevant. Key features include:</p>
        <ul>
          <li>Personalized learning plans based on diagnostic assessments</li>
          <li>Interactive lessons with real-world applications</li>
          <li>Small group instruction (max 6 students per group)</li>
          <li>Weekly progress reports for parents</li>
          <li>Supplemental online practice platform</li>
        </ul>
        <p><strong>Schedule:</strong> Twice weekly, 90-minute sessions</p>
        <p><strong>Tuition:</strong> $299/month</p>`
      },
      'science-middle': {
        title: 'Science Explorers (5th-8th Grade)',
        content: `<p>Ignite curiosity with our hands-on science program that makes complex concepts accessible. Program highlights:</p>
        <ul>
          <li>Experiment-based learning with take-home lab kits</li>
          <li>Covers life, physical, and earth sciences</li>
          <li>STEM project challenges each semester</li>
          <li>Science fair preparation support</li>
          <li>Virtual reality field trips</li>
        </ul>
        <p><strong>Schedule:</strong> Once weekly, 2-hour lab sessions</p>
        <p><strong>Tuition:</strong> $249/month (includes all materials)</p>`
      },
      'math-high': {
        title: 'Advanced Mathematics (9th-12th Grade)',
        content: `<p>Comprehensive high school math courses taught by subject experts. We offer:</p>
        <ul>
          <li>Algebra I & II</li>
          <li>Geometry</li>
          <li>Trigonometry</li>
          <li>Pre-Calculus</li>
          <li>Calculus (AB & BC)</li>
          <li>Statistics</li>
        </ul>
        <p>All courses align with Common Core and state standards while preparing students for college-level math.</p>
        <p><strong>Schedule:</strong> Flexible scheduling options available</p>
        <p><strong>Tuition:</strong> $349/month per course</p>`
      },
      'sat-prep': {
        title: 'SAT Preparation Program',
        content: `<p>Our proven SAT prep program has helped students increase their scores by an average of 200 points. Features include:</p>
        <ul>
          <li>Full-length diagnostic test with detailed analysis</li>
          <li>16-week intensive program (4 hours/week)</li>
          <li>Small group classes (max 8 students)</li>
          <li>5 official practice tests with proctored conditions</li>
          <li>Math and verbal strategy sessions</li>
          <li>Essay writing workshop</li>
        </ul>
        <p><strong>Next Session:</strong> Starts September 5th</p>
        <p><strong>Tuition:</strong> $999 (includes all materials and tests)</p>`
      }
    };
    
    // Modal functions
    function showProgramDetails(programId) {
      const program = programData[programId] || {
        title: 'Program Details',
        content: '<p>Detailed information about this program is coming soon. Please contact us for more details.</p>'
      };
      
      document.getElementById('modalTitle').textContent = program.title;
      document.getElementById('modalContent').innerHTML = program.content;
      document.getElementById('programModal').style.display = 'flex';
    }
    
    function closeModal() {
      document.getElementById('programModal').style.display = 'none';
    }
    
    // Close modal when clicking outside
    window.onclick = function(event) {
      const modal = document.getElementById('programModal');
